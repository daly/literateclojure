public Object throwArity(int n){
    String name = getClass().getSimpleName();
    int suffix = name.lastIndexOf("__");
    throw new ArityException(n, 
        (suffix == -1 
                ? name 
                : name.substring(0, suffix)).replace('_', '-'));
}

static public Class loadClassForName(String name)
 throws ClassNotFoundException{
    try
        {
        Class.forName(name, false, baseLoader());
        }
    catch(ClassNotFoundException e)
        {
        return null;
        }
    return Class.forName(name, true, baseLoader());
}


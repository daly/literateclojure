package clojure.lang;

public class Delay implements IDeref{
Object val;
IFn fn;

public Delay(IFn fn){
    this.fn = fn;
    this.val = null;
}

static public Object force(Object x) throws Exception{
    return (x instanceof Delay) ?
           ((Delay) x).deref()
           : x;
}

synchronized public Object deref() throws Exception{
    if(fn != null)
        {
        val = fn.invoke();
        fn = null;
        }
    return val;
}

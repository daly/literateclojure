protected static ISeq ontoArrayPrepend(Object[] array, 
                                       Object... args){
    ISeq ret = ArraySeq.create(array);
    for(int i = args.length - 1; i >= 0; --i)
        ret = RT.cons(args[i], ret);
    return ret;
}

protected static ISeq findKey(Object key, ISeq args){
    while(args != null)
        {
        if(key == args.first())
            return args.next();
        args = RT.next(args);
        args = RT.next(args);
        }
    return null;
}



package clojure.lang;

public class Range extends ASeq implements IReduce, Counted{
final int end;
final int n;

public Range(int start, int end){
    this.end = end;
    this.n = start;
}

public Range(IPersistentMap meta, int start, int end){
    super(meta);
    this.end = end;
    this.n = start;
}

public Obj withMeta(IPersistentMap meta){
    if(meta == meta())
        return this;
    return new Range(meta(), end, n);
}

public Object first(){
    return n;
}

public ISeq next(){
    if(n < end-1)
        return new Range(_meta, n + 1, end);
    return null;
}

public Object reduce(IFn f) throws Exception{
    Object ret = n;
    for(int x = n+1;x < end;x++)
        ret = f.invoke(ret, x);
    return ret;
}

public Object reduce(IFn f, Object start) throws Exception{
    Object ret = f.invoke(start,n);
    for(int x = n+1;x < end;x++)
        ret = f.invoke(ret, x);
    return ret;
}

public int count() {
    return end - n;
    }

}

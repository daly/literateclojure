 { \pstree{\Tr{\psframebox{$\ ||\ $}}}
   { \Tr{\psframebox{23}}
     \pstree{\Tr{\psframebox{$\ ||\ $}}}
     { \Tr{\psframebox{47}}
       \Tr{\psframebox{$/$}}
     }
   }
